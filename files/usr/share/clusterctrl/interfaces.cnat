allow-hotplug ethpi211
iface ethpi211 inet manual
        pre-up ifup br0
        pre-up brctl addif br0 ethpi211
	up ip link set dev ethpi211 up

# usbboot
# Internal network (VLAN 10)
allow-hotplug ethupi211.10
iface ethupi211.10 inet manual
        pre-up ifup brint
        pre-up brctl addif brint ethupi211.10

# External network (untagged)
allow-hotplug ethupi211
iface ethupi211 inet manual
	pre-up ifup br0
	pre-up brctl addif br0 ethupi211
	up ip link set dev ethupi211 up
	post-up ip link add link ethupi211 name ethupi211.10 type vlan id 10

